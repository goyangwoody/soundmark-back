    "/api/v1/users/{user_id}/followers": {
      "get": {
        "tags": [
          "Users & Follow"
        ],
        "summary": "Get User Followers",
        "description": "Get list of users following this user\n\nArgs:\n    user_id: ID of the user\n    limit: Maximum number of results (1-100)\n    offset: Number of results to skip\n\nReturns:\n    List of followers and total count",
        "operationId": "get_user_followers_api_v1_users__user_id__followers_get",
        "parameters": [
          {
            "name": "user_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "User Id"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1,
              "default": 50,
              "title": "Limit"
            }
          },
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 0,
              "default": 0,
              "title": "Offset"
            }
