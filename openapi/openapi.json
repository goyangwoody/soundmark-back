  "components": {
    "schemas": {
      "ActiveRecommendation": {
        "properties": {
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "lat": {
            "type": "number",
            "title": "Lat"
          },
          "lng": {
            "type": "number",
            "title": "Lng"
          },
          "distance_meters": {
            "type": "number",
            "title": "Distance Meters"
          },
          "track": {
            "$ref": "#/components/schemas/TrackResponse"
          },
          "user": {
            "$ref": "#/components/schemas/UserResponse"
          },
          "message": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Message"
          },
          "reactions": {
            "additionalProperties": {
              "type": "integer"
            },
            "type": "object",
            "title": "Reactions",
            "description": "Emoji reactions with counts"
          },
          "user_reaction": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "User Reaction",
            "description": "Current user's reaction emoji"
          }
        },
        "type": "object",
        "required": [
          "id",
          "lat",
          "lng",
          "distance_meters",
          "track",
          "user"
        ],
        "title": "ActiveRecommendation",
        "description": "Individual active recommendation (within 200m)"
      },
      "FollowResponse": {
        "properties": {
          "success": {
            "type": "boolean",
            "title": "Success"
          },
          "message": {
            "type": "string",
            "title": "Message"
          },
          "follower_count": {
            "type": "integer",
            "title": "Follower Count"
          }
        },
        "type": "object",
        "required": [
          "success",
          "message",
          "follower_count"
        ],
        "title": "FollowResponse",
        "description": "Response for follow/unfollow actions"
      },
